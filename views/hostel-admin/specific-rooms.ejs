  <script>
    $(document).ready(function(){
      setInterval(function(){
        $.ajax({
            url: "/getData",
            type: "POST",
            data: {
                'id':"<%= data['hname'] %>"
            },
            success: function(data){
              keys=Object.keys(data.data.roomsGF[0])
              let avail=0;
              for(let i=0;i<keys.length;i++){     
                if(data.data1.some(processing=>processing.hostelname===data.data['hname'] && processing.roomno===keys[i])){ 
                  
                  $('#'+keys[i]).css("background-color","yellow");
                }
                else{
                 
                  $('#'+keys[i]).css("background-color","white");
                }
                avail+=(data.data['totalCount']-data.data['roomsGF'][0][keys[i]])
                $('#'+data.data['hname']+keys[i]).html("<span style='color:red'>Availability : </span>"+ data.data.roomsGF[0][keys[i]]+"  |  "+data.data.totalCount )
              }
              keys=Object.keys(data.data.roomsFF[0])
              for(let i=0;i<keys.length;i++){     
                if(data.data1.some(processing=>processing.hostelname===data.data['hname'] && processing.roomno===keys[i])){ 
                  
                  $('#'+keys[i]).css("background-color","yellow");
                }
                else{
                 
                  $('#'+keys[i]).css("background-color","white");
                }
                avail+=(data.data['totalCount']-data.data['roomsFF'][0][keys[i]])
                $('#'+data.data['hname']+keys[i]).html("<span style='color:red'>Availability : </span>"+ data.data.roomsFF[0][keys[i]]+"  |  "+data.data.totalCount )
              }
              keys=Object.keys(data.data.roomsSF[0])
              for(let i=0;i<keys.length;i++){     
                if(data.data1.some(processing=>processing.hostelname===data.data['hname'] && processing.roomno===keys[i])){ 
                  
                  $('#'+keys[i]).css("background-color","yellow");
                }
                else{
               
                  $('#'+keys[i]).css("background-color","white");
                }
                avail+=(data.data['totalCount']-data.data['roomsSF'][0][keys[i]])
                $('#'+data.data['hname']+keys[i]).html("<span style='color:red'>Availability : </span>"+ data.data.roomsSF[0][keys[i]]+"  |  "+data.data.totalCount )
              }
              keys=Object.keys(data.data.roomsTF[0])
              for(let i=0;i<keys.length;i++){     
                if(data.data1.some(processing=>processing.hostelname===data.data['hname'] && processing.roomno===keys[i])){ 
            
                  $('#'+keys[i]).css("background-color","yellow");
                }
                else{
                 
                  $('#'+keys[i]).css("background-color","white");
                }
                avail+=(data.data['totalCount']-data.data['roomsTF'][0][keys[i]])
                $('#'+data.data['hname']+keys[i]).html("<span style='color:red'>Availability : </span>"+ data.data.roomsTF[0][keys[i]]+"  |  "+data.data.totalCount )
              }
              $('#avail').html("<span class='badge bg-primary bx-pull-right'>Availability : "+ avail+"</span>")
            }
        });
      },1000)
    })
  </script>
  
