    <section class="section dashboard">
      <div class="row">
        <!-- Left side columns -->
        <div class="col-lg-12">
          <div class="row">
            <div class="col-12">
              <div class="card recent-sales overflow-auto">
                <div class="card-body">
                  <h5 class="card-title">Hostel Students Details </h5>
                  <h5 class=""><i></i></h5>
                  <table class="table table-borderless datatable">
                    <thead>
                      <tr>
                        <th scope="col">Student-ID</th>
                        <th scope="col">Student Name</th>
                        <th scope="col">E-mail</th>
                        <th scope="col">Phone</th>
                        <th scope="col">College</th>
                        <th scope="col">Gender</th>
                        <th scope="col">Hostel</th>
                        <th scope="col">Room Alloted</th>
                        <th scope="col">Delete</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for(let i=0;i<data.length;i++){ %>
                      <tr>
                        <td>KD1123</td>
                        <td><%= data[i]['name'] %></td>
                        <td><%= data[i]['email'] %></td>
                        <td><%= data[i]['phone'] %></td>
                        <td><%= data[i]['college'] %></td>
                        <td><%= data[i]['gender'] %></td>
                        <td><%= data[i]['hostelname'] %></td>
                        <td><%= data[i]['roomno'] %></td>
                        <td><a href="/eventDelete/<%= data[i]['_id'] %>"><button class="btn btn-danger">Delete</button></a></td>
                      </tr>
                      <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div><!-- End Recent Sales -->
          </div>
        </div><!-- End Left side columns -->

        <% if(data.length < count) { %>
            <div class="card">
                <div class="card-body">
                  <h5 class="card-title">Add Students</h5>
                  <form class="row g-3" action="/hostelBookingManual" method="post">
                    <div class="col-md-12">
                      <label for="inputName5" class="form-label">Student E-mail</label>
                      <input type="text" name="email" class="form-control" value="" id="inputName5">
                    </div>
                    <div class="col-md-6">
                      <label for="inputEmail5" class="form-label">Name</label>
                      <input type="text" name="name" class="form-control" value="" id="inputEmail5">
                    </div>
                    <div class="col-md-6">
                      <label for="inputPassword5" class="form-label">Phone No</label>
                      <input type="text" name="phone" class="form-control" value="" id="inputPassword5">
                    </div>
                    <div class="col-md-6">
                        <label for="inputPassword5" class="form-label">College</label>
                        <input type="text" name="clg" class="form-control" value="" id="inputPassword5">
                      </div>
                    
                    <div class="col-md-6">
                      <label for="inputAddress5"  class="form-label">Gender</label>
                      <input type="text" name="gender" class="form-control"  value="<%= gender %>" id="inputAddres5s" placeholder="" hidden>
                      <input type="text" name="gender" class="form-control"  value="<%= gender %>" id="inputAddres5s" placeholder="" disabled>
                    </div>
                    
                    <div class="col-md-6">
                      <label for="inputAddress2" class="form-label">Hostel-Name</label>
                      <input type="text" name="hname" class="form-control" id="inputAddress2" value="<%= hname %>" hidden>
                      <input type="text" name="hname" class="form-control" id="inputAddress2" value="<%= hname %>" disabled>
                    </div>
                    <div class="col-md-6">
                        <label for="inputAddress5" class="form-label">Hostel-room no</label>
                        <input type="text" name="hroom" class="form-control" id="inputAddres5s" value="<%= room %>" hidden>
                        <input type="text" name="hroom" class="form-control" id="inputAddres5s" value="<%= room %>" disabled>
                      </div>

                      <div class="col-md-6">
                        <input type="text" name="rfloor" class="form-control" id="inputAddres5s" value="<%= floor %>" hidden>
                        <input type="text" name="count" class="form-control" id="inputAddres5s" value="<%= count %>" hidden>
                      </div>
                    
                    <div class="text-center">
                      <button type="submit" class="btn btn-primary">Submit</button>
                      <button type="reset" class="btn btn-secondary">Reset</button>
                    </div>
                  </form><!-- End Multi Columns Form -->
    
                </div>
              </div>
            </div>
            <% } %>
       
        
