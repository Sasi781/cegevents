          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Add Event</h5>

              <!-- Multi Columns Form -->
              <form class="row g-3" action="/events" method="post">
                <div class="col-md-2">
                    <label for="inputZip" class="form-label">EventID</label>
                    <input type="number" class="form-control" id="inputZip" value="<%= ID %>" disabled>
                    <input type="number" name="eventid" value="<%= ID %>" hidden>
                  </div>
                <div class="col-md-10">
                  <label for="inputName5" class="form-label">Event Name</label>
                  <input type="text" name="ename" class="form-control" id="inputName5">
                </div>
                <div class="col-md-6">
                  <label for="inputEmail5" class="form-label">Date</label>
                  <input type="date" name="date" class="form-control" id="inputEmail5">
                </div>
                <div class="col-md-6">
                  <label for="inputPassword5" class="form-label">Venue</label>
                  <input type="text" name="venue" class="form-control" id="inputPassword5">
                </div>
                <div class="col-md-6">
                    <label for="inputPassword5" class="form-label">Cash Price</label>
                    <input type="text" name="price" class="form-control" id="inputPassword5">
                  </div>
                
                  <div class="col-md-6">
                    <label for="inputState" class="form-label">Domain</label>
                    <select id="inputState" name="domain" class="form-select">
                      <option selected>Choose...</option>
                      <option value="engineering">Engineering</option>
                      <option value="management">Management</option>
                      <option value="quiz">Quiz</option>
                      <option value="coding">Coding</option>
                      <option value="extravaganza">Extravaganza</option>
                      <option value="robotics">Robotics</option>
                      <option value="paper">Paper Presentation</option>
                    </select>
                  </div>  
                <div class="col-md-6">
                  <label for="inputAddress5"  class="form-label">Event-ADMIN Email-1</label>
                  <input type="text" name="email1" class="form-control" id="inputAddres5s" placeholder="">
                </div>
                <div class="col-md-6">
                  <label for="inputAddress2" class="form-label">Event-ADMIN Password-1</label>
                  <input type="text" name="pass1" class="form-control" id="inputAddress2" placeholder="">
                </div>
                <div class="col-md-6">
                    <label for="inputAddress5" class="form-label">Event-ADMIN Email-2</label>
                    <input type="text" name="email2" class="form-control" id="inputAddres5s" placeholder="">
                  </div>
                  <div class="col-md-6">
                    <label for="inputAddress2" class="form-label">Event-ADMIN Password-2</label>
                    <input type="text" name="pass2" class="form-control" id="inputAddress2" placeholder="">
                  </div>

                  <div class="col-12">
                    <label for="inputAddress5" class="form-label">Add Description</label>
                    <input type="text" name="desc" class="form-control" id="inputAddres5s" placeholder="">
                  </div>
                <div class="text-center">
                  <button type="submit" class="btn btn-primary">Submit</button>
                  <button type="reset" class="btn btn-secondary">Reset</button>
                </div>
              </form><!-- End Multi Columns Form -->

